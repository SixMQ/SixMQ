    "require": {
        "php": ">=7.0.0",
        "yurunsoft/imi": "dev-dev",
        "sixmq/sixmq-struct": "dev-master",
    },
    "require-dev": {
        "swoft/swoole-ide-helper": "~2.0"
    },
    "repositories": [
        {
